{
  "name": "core-s3",
  "version": "1.0.0",
  "description": "ğŸª¶ A super lightweight S3 client, no-dependency, ideal for edges like @cloudflare @aws @Azure @GoogleCloudPlatform @ceph @minio written in TypeScript",
  "keywords": [
    "s3",
    "s3 client",
    "s3 sdk",
    "b2",
    "b2 client",
    "r2",
    "r2 client",
    "cloudflare",
    "cloudflare r2",
    "AWS S3",
    "Azure Blob Storage",
    "Google Cloud Storage",
    "Ceph",
    "mibion",
    "backblaze"
  ],
  "homepage": "https://github.com/good-lly/core-s3",
  "bugs": {
    "url": "https://github.com/good-lly/core-s3/issues"
  },
  "author": "good-lly",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/good-lly/core-s3.git"
  },
  "files": [
    "./dist/",
    "./src/",
    "./types/",
    "./package.json",
    "./README.md"
  ],
  "type": "module",
  "scripts": {
    "tsc": "tsc",
    "type-check": "tsc --noEmit --emitDeclarationOnly false",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier -w src",
    "format-check": "prettier --list-different src",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run tsc"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "engines": {
    "node": ">=20"
  },
  "engineStrict": true,
  "targets": {
    "node": 16,
    "edge": true,
    "worker": true,
    "bun": true,
    "browser": false
  },
  "devDependencies": {
    "@types/node": "^22.13.5",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.5.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.6",
    "typescript": "^5.7.3"
  }
}
