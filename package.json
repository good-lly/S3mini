{
  "name": "S3mini",
  "version": "0.0.1",
  "description": "ðŸª¶ A super lightweight S3 client, no-dependency, ideal for edges like @cloudflare @aws @Azure @GoogleCloudPlatform @ceph @minio written in TypeScript",
  "homepage": "https://github.com/good-lly/S3mini",
  "bugs": {
    "url": "https://github.com/good-lly/S3mini/issues"
  },
  "author": "good-lly",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/good-lly/S3mini.git"
  },
  "files": [
    "./dist/",
    "./src/",
    "./types/",
    "./package.json",
    "./README.md"
  ],
  "type": "module",
  "scripts": {
    "tsc": "tsc",
    "typecheck": "tsc --noEmit",
    "test:e2e": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config jest.config.js --verbose",
    "test:perf": "node ./tests/perf/performance.test.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier -w src",
    "formatcheck": "prettier --list-different src",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run typecheck && rollup -c",
    "dev": "npm run clean && npm run typecheck -w && rollup -c -w"
  },
  "main": "dist/S3mini.min.js",
  "module": "dist/S3mini.js",
  "types": "dist/S3mini.d.ts",
  "exports": {
    "import": {
      "development": "./dist/S3mini.js",
      "default": "./dist/S3mini.min.js"
    },
    "require": "./dist/S3mini.min.js",
    "types": "./dist/S3mini.d.ts"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=20"
  },
  "engineStrict": true,
  "targets": {
    "node": 20,
    "edge": true,
    "worker": true,
    "bun": true,
    "browser": false
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.802.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.26.0",
    "@monstermann/tinybench-pretty-printer": "^0.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/node": "^22.15.14",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.0.1",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "minio": "^8.0.5",
    "prettier": "^3.5.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.34.8",
    "rollup-plugin-dts": "^6.1.1",
    "tinybench": "^4.0.1",
    "ts-jest": "^29.2.6",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3"
  },
  "prettier": {
    "bracketSpacing": true,
    "bracketSameLine": false,
    "singleQuote": true,
    "semi": true,
    "endOfLine": "lf",
    "tabWidth": 2,
    "trailingComma": "all",
    "arrowParens": "avoid",
    "printWidth": 120
  },
  "keywords": [
    "s3",
    "s3 client",
    "s3 sdk",
    "b2",
    "b2 client",
    "r2",
    "r2 client",
    "cloudflare",
    "cloudflare r2",
    "AWS S3",
    "Azure Blob Storage",
    "Google Cloud Storage",
    "Ceph",
    "mibion",
    "backblaze"
  ]
}
